<div class="col-md-6 offset-md-3">
    <div class="card">
        <div class="card-header">
            <h1>Productos Webhook</h1>
        </div>
        <div class="card-body">
            {{#if data}}
            <ul class="list-group">
                {{#each data}}
                <li class="list-group-item d-flex justify-content-between align-items-center">
                    {{@key}} ||
                    {{nombre}} ||
                    {{stock}} ||
                    {{descripcion}} ||

                </li>
                {{/each}}


            </ul>
            {{else}}
            <p1>No hay datos</p1>

            {{/if}}
        </div>
